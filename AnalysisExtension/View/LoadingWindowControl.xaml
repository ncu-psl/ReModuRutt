<UserControl x:Class="AnalysisExtension.View.LoadingWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AnalysisExtension.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="350">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <ProgressBar x:Name="loadingProgessBar" Height="10" Width="200" HorizontalAlignment="Center"/>
            <TextBlock x:Name="persentText" Height="30" Width="30" HorizontalAlignment="Right"/>
        </StackPanel>
            
    </Grid>
</UserControl>
